    Width="90" VerticalAlignment="Top" Margin="695,130,0,0" HorizontalAlignment="Left" Foreground="White" Content="8" BorderBrush="White" Background="#FF000C3B"/>
        <Button
    Width="90" VerticalAlignment="Top" Margin="596,172,0,0" HorizontalAlignment="Left" Foreground="White" Content="I" Click="Button_Click" BorderBrush="White" Background="#FF000C3B"/>
        <Button
    Width="90" VerticalAlignment="Top" Margin="695,151,0,0" HorizontalAlignment="Left" Foreground="White" Content="M" BorderBrush="White" Background="#FF000C3B"/>
        <Button
    Width="90" VerticalAlignment="Top" Margin="695,172,0,0" HorizontalAlignment="Left" Foreground="White" Content="Все модели" BorderBrush="White" Background="#FF000C3B" FontSize="10" Height="20"/>
