    Width="90" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Margin="596,109,0,0" HorizontalAlignment="Left" Foreground="White" Content="5" Click="Button_Click" BorderBrush="White" Background="#FF000C3B"/>
