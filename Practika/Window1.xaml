    Width="90" VerticalAlignment="Top" Margin="596,67,0,0" HorizontalAlignment="Left" Foreground="White" Content="X" Click="Button_Click" BorderBrush="White" Background="#FF000C3B"/>
        <Button
    Width="90" VerticalAlignment="Top" Margin="697,67,0,0" HorizontalAlignment="Left" Foreground="White" Content="2" BorderBrush="White" Background="#FF000C3B"/>
        <Rectangle
    VerticalAlignment="Top" Margin="590,35,0,0" Stroke="#FF000C3B" Width="200" Height="167" HorizontalAlignment="Left" RadiusX="15" RadiusY="15"/>
